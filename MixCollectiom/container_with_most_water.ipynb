   "source": [
    "def maxArea(self, height: List[int]) -> int:\n",
    "\tmaxarea = 0\n",
    "\tl = 0\n",
    "\tr = len(height)-1\n",
    "\twhile(l<r):\n",
    "\t\tmaxarea = max(maxarea, min(height[l],height[r])*(r-l))\n",
    "\t\tif(height[l]<height[r]):\n",
    "\t\t\tl+=1\n",
    "\t\telse:\n",
    "\t\t\tr-=1\n",
    "\treturn maxarea"
   ]
  }
